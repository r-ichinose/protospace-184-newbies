<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), prototypes_path(prototype.id) %>
  <div class="card__body">
    <%= link_to prototype.title, prototypes_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= link_to prototype.catch_copy, prototypes_path(prototype.id) %>
    </p>
    <%= link_to "by #{prototype.user.name}", prototypes_path(prototype.id), class: :card__user %>
  </div>
</div>